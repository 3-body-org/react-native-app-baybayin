{
  "expo": {
    "platforms": ["ios", "android", "web"],
    "scheme": "baynolohiya",
    "name": "baynolohiya",
    "slug": "baynolohiya",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.erudite098.baynolohiya"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.DOWNLOAD_WITHOUT_NOTIFICATION",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "package": "com.erudite098.baynolohiya"
    },
    "web": {},
    "plugins": [
      "@config-plugins/react-native-blob-util",
      "@config-plugins/react-native-pdf",
      "expo-asset",
      "expo-router"
    ],
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "3e3249ea-9b91-4292-9fc1-5103cc0596fc"
      }
    }
  }
}
